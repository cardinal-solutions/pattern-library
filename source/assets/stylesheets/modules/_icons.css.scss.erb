%icons {
  font-family: "cardinal Icons";
}
<% data.icons.each do |icon| %>
  .icon-<%= icon.name.downcase.tr(" ", "-") %>:before {
    @extend %icons;
    content: "\<%= icon.entity %>";
  }
<% end %>
